{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "jsast",
          "version": "1.0.0",
          "informationUri": "https://github.com/belos-street/jsast",
          "rules": [
            {
              "id": "no-console-log",
              "name": "no-console-log",
              "shortDescription": {
                "text": "Disallows the use of console.log"
              },
              "fullDescription": {
                "text": "Disallows the use of console.log"
              },
              "defaultConfiguration": {
                "level": "note"
              }
            },
            {
              "id": "no-var",
              "name": "no-var",
              "shortDescription": {
                "text": "Disallows the use of var keyword"
              },
              "fullDescription": {
                "text": "Disallows the use of var keyword"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            },
            {
              "id": "command-injection",
              "name": "command-injection",
              "shortDescription": {
                "text": "Detects command injection vulnerabilities"
              },
              "fullDescription": {
                "text": "Detects command injection vulnerabilities"
              },
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "no-unsafe-spawn",
              "name": "no-unsafe-spawn",
              "shortDescription": {
                "text": "Detects unsafe spawn function calls"
              },
              "fullDescription": {
                "text": "Detects unsafe spawn function calls"
              },
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "no-unsafe-shell",
              "name": "no-unsafe-shell",
              "shortDescription": {
                "text": "Detects command execution with shell option enabled"
              },
              "fullDescription": {
                "text": "Detects command execution with shell option enabled"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            },
            {
              "id": "detect-sql-injection",
              "name": "detect-sql-injection",
              "shortDescription": {
                "text": "Detects SQL injection vulnerabilities"
              },
              "fullDescription": {
                "text": "Detects SQL injection vulnerabilities"
              },
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "avoid-raw-sql",
              "name": "avoid-raw-sql",
              "shortDescription": {
                "text": "Avoid using raw SQL strings, use ORM query builders instead"
              },
              "fullDescription": {
                "text": "Avoid using raw SQL strings, use ORM query builders instead"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            },
            {
              "id": "detect-mongodb-injection",
              "name": "detect-mongodb-injection",
              "shortDescription": {
                "text": "Detects MongoDB injection vulnerabilities"
              },
              "fullDescription": {
                "text": "Detects MongoDB injection vulnerabilities"
              },
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "avoid-dangerously-set-innerhtml",
              "name": "avoid-dangerously-set-innerhtml",
              "shortDescription": {
                "text": "Avoid using innerHTML or dangerouslySetInnerHTML with untrusted input"
              },
              "fullDescription": {
                "text": "Avoid using innerHTML or dangerouslySetInnerHTML with untrusted input"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            },
            {
              "id": "detect-path-traversal",
              "name": "detect-path-traversal",
              "shortDescription": {
                "text": "Detects path traversal vulnerabilities"
              },
              "fullDescription": {
                "text": "Detects path traversal vulnerabilities"
              },
              "defaultConfiguration": {
                "level": "error"
              }
            },
            {
              "id": "avoid-unsafe-fs-access",
              "name": "avoid-unsafe-fs-access",
              "shortDescription": {
                "text": "Avoid unsafe file system access with dynamically concatenated paths"
              },
              "fullDescription": {
                "text": "Avoid unsafe file system access with dynamically concatenated paths"
              },
              "defaultConfiguration": {
                "level": "warning"
              }
            }
          ]
        }
      },
      "results": []
    }
  ]
}